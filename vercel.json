{
    "version": 2,
    "name": "cal-app-staging",
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "server/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "server/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.ts"
        },
        {
            "src": "/assets/(.*)",
            "dest": "client/dist/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "client/dist/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "iad1"
    ]
}